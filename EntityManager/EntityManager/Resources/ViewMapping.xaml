<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:EntityManager"
                    xmlns:pages="clr-namespace:EntityManager.Pages"
                    xmlns:viewModels="clr-namespace:EntityManager.ViewModels">
    <DataTemplate DataType="{x:Type viewModels:MonstersViewModel}">
        <pages:MonsterPage/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:SpellsViewModel}">
        <pages:SpellsPage/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:EquipmentViewModel}">
        <pages:EquipmentPage/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:RacesViewModel}">
        <pages:RacesPage/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:ClassesViewModel}">
        <pages:ClassesPage/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:BackgroundsViewModel}">
        <pages:BackgroundsPage/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:ConditionsViewModel}">
        <pages:ConditionPage/>
    </DataTemplate>
</ResourceDictionary>